<script language="javascript">

// onload handler from frame
function resizeFrame()
{
	try {
		var f = window.frames["storeLocator"];
		var actualHeight = f.document.body.scrollHeight;
		I2O_pageLoaded(actualHeight);
	} catch(e) {} // older browsers do not support scrollHeight
}

// notification from an inner main page that it has been loaded:
// adjust the storeLocator iframe height
function I2O_pageLoaded( frameActualHeight )
{
	frameActualHeight += 0;
	
	if (frameActualHeight < 510 )
		frameActualHeight = 510;
		
	var frame = document.getElementById("storeLocator");
	if (frame != null )
	{
		I2O_scrollToTop();
		
		frame.height = frameActualHeight;
		frame.style.display="block";			
	}
}

// called from "back to top" link
function I2O_scrollToTop()
{
	try{ document.body.scrollTop=0; } catch(e){};	
}


</script>
