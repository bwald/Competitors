            <area shape="rect" coords="350,206,386,255" href="javascript:btnCntrs = 3; javascript:imageSwitch();" />
            <area shape="rect" coords="385,206,422,255" href="javascript:btnCntrs = 0; javascript:imageSwitch();" />
            <area shape="rect" coords="421,206,458,254" href="javascript:btnCntrs = 1; javascript:imageSwitch();" />
            <area shape="rect" coords="457,206,494,255" href="javascript:btnCntrs = 2; javascript:imageSwitch();" />
            <area shape="rect" coords="493,225,547,253" href="javascript:makeThisStayStopped++; javascript:stopSwitch();" alt="Pause/Play" />
            <area shape="rect" coords="278,140,441,188" href="" id="mainAdLink2" alt="Sale!" onClick="_gaq.push(['_trackEvent', 'WeeklyAd', 'Clicked', 'Slides']);" />
