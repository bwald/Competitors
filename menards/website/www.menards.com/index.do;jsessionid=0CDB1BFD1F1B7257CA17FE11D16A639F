<script>
  var mainImage = new Array();
  var mainBody1altText = new Array();
  var mainBody1Links = new Array();
  var mainBody2altText = new Array();
  var mainBody2Links = new Array();
  var mainBody3altText = new Array();
  var mainBody3Links = new Array();

</script>

<script type="text/javascript" language="javascript" src="web/managed/index/variables.txt"></script>

<script>
  var btnCntrs = 3;
  var t = "";
  var timeDelay = 5000;
  var timeDelayForFade = 3;
  var numberOfImages = mainImage.length;
  var makeThisStayStopped = 0;
  
  var preLoad = new Array();
  for (i = 0; i < numberOfImages; i++){
  	preLoad[i] = new Image();
	preLoad[i].src = "web/managed/index/" + mainImage[i];
  }

function imageSwitch(){
	if (t != "") {
      clearTimeout(t);
	}
   btnCntrs = btnCntrs + 1;
   if (btnCntrs > (numberOfImages-1)) btnCntrs=0;
   if (document.all){
      document.images.mainAdImage.style.filter="blendTrans(duration=2)";
      document.images.mainAdImage.style.filter="blendTrans(duration=timeDelayForFade)";
      document.images.mainAdImage.filters.blendTrans.Apply();     
   }
     document.images.mainAdImage.src = preLoad[btnCntrs].src;

   if (document.all){
      document.images.mainAdImage.filters.blendTrans.Play();
   }
	  document.getElementById('mainAdLink1').href = mainBody1Links[btnCntrs];
	  document.getElementById('mainAdLink1').alt = mainBody1altText[btnCntrs];
	  document.getElementById('mainAdLink2').href = mainBody2Links[btnCntrs];
	  document.getElementById('mainAdLink2').alt = mainBody2altText[btnCntrs];
	  document.getElementById('mainAdLink3').href = mainBody3Links[btnCntrs];
	  document.getElementById('mainAdLink3').alt = mainBody3altText[btnCntrs];

	if (makeThisStayStopped == 1) {
	   t = setTimeout('stopSwitch()', timeDelay);
	} else {
	   makeThisStayStopped = 0;
	   t = setTimeout('imageSwitch()', timeDelay);
	}
}

function stopSwitch() {
	if (t != "") {
      clearTimeout(t);
	}
	if (makeThisStayStopped != 1) {
	   makeThisStayStopped = 0;
	   imageSwitch();
	} else {
		t = setTimeout("stopSwitch()", timeDelay);
	}
}
	setTimeout("imageSwitch()", 2000);

</script>

