function trim(string) {
  var temp = string;
  while(temp.indexOf(" ") == 0) {
    temp = temp.substring(1, temp.length);
  }
  while(temp.lastIndexOf(" ") == temp.length-1 && temp.length > 0) {
    temp = temp.substring(0, temp.length-1);
  }
