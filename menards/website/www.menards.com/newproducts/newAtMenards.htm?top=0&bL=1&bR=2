<script type="text/javascript">

stepcarousel.setup({
	galleryid: 'mygallery', //id of carousel DIV
	beltclass: 'belt', //class of inner "belt" DIV containing all the panel DIVs
	panelclass: 'panel', //class of panel DIVs each holding content
	statusvars: ['startp', 'endp', 'totalp'], //register 3 variables that contain current panel (start), current panel (last), and total panels
	contenttype: ['ajax', 'page.htm'] //content setting ['inline'] or ['external', 'path_to_external_file']
})

var xmldoc;
var x;
var url = "xml/products.xml";

try
	{
		if (window.ActiveXObject)
		{
			var errorHappendHere = "Check Browser and security settings";
			xmlDoc = new ActiveXObject("Microsoft.XMLDOM");
			xmlDoc.async=false;
			xmlDoc.load(url);
		}
		else if(window.XMLHttpRequest)
		{
			var errorHappendHere = "Error handling XMLHttpRequest request";
			var d = new XMLHttpRequest();
			d.open("GET", url, false);
			d.send(null);
			xmlDoc=d.responseXML;
		} else {
			var errorHappendHere = "Error.";
			xmlDoc = document.implementation.createDocument("","",null);
			xmlDoc.async=false;
			xmlDoc.load(url);
		}
	}	
	catch(e)
	{
		alert(errorHappendHere);
	}

x = xmlDoc.getElementsByTagName("PRODUCT");

function displayInformation(top, bLeft, bRight) {

topMiscTxt='';
blMiscTxt='';
brMiscTxt='';

for (i=0; i<x[top].getElementsByTagName('MISCTXT')[0].getElementsByTagName('SKU').length; i++)
{
	topMiscTxt += "<span style='font-weight:bold;'>" + x[top].getElementsByTagName('MISCTXT')[0].getElementsByTagName('DETAIL')[i].childNodes[0].nodeValue + "</span><br/>";
	topMiscTxt += "<span style='font-size:10px;'>" + x[top].getElementsByTagName('MISCTXT')[0].getElementsByTagName('SKU')[i].childNodes[0].nodeValue + "</span><br/>";
	topMiscTxt += "<span style='font-size:14px;font-weight:bold;'>" + x[top].getElementsByTagName('MISCTXT')[0].getElementsByTagName('PRICE')[i].childNodes[0].nodeValue + "</span> ";
	topMiscTxt += "<span style='font-size:10px;'>" + x[top].getElementsByTagName('MISCTXT')[0].getElementsByTagName('UNIT')[i].childNodes[0].nodeValue + "</span><br/><br/>";

}
for (j=0; j<x[bLeft].getElementsByTagName('MISCTXT')[0].getElementsByTagName('SKU').length; j++)
{
	blMiscTxt += "<span style='font-weight:bold;'>" + x[bLeft].getElementsByTagName('MISCTXT')[0].getElementsByTagName('DETAIL')[j].childNodes[0].nodeValue + "</span><br/>";
	blMiscTxt += "<span style='font-size:10px;'>" + x[bLeft].getElementsByTagName('MISCTXT')[0].getElementsByTagName('SKU')[j].childNodes[0].nodeValue + "</span><br/>";
	blMiscTxt += "<span style='font-size:14px;font-weight:bold;'>" + x[bLeft].getElementsByTagName('MISCTXT')[0].getElementsByTagName('PRICE')[j].childNodes[0].nodeValue + "</span> ";
	blMiscTxt += "<span style='font-size:10px;'>" + x[bLeft].getElementsByTagName('MISCTXT')[0].getElementsByTagName('UNIT')[j].childNodes[0].nodeValue + "</span><br/><br/>";

}
for (k=0; k<x[bRight].getElementsByTagName('MISCTXT')[0].getElementsByTagName('SKU').length; k++)
{
	brMiscTxt += "<span style='font-weight:bold;'>" + x[bRight].getElementsByTagName('MISCTXT')[0].getElementsByTagName('DETAIL')[k].childNodes[0].nodeValue + "</span><br/>";
	brMiscTxt += "<span style='font-size:10px;'>" + x[bRight].getElementsByTagName('MISCTXT')[0].getElementsByTagName('SKU')[k].childNodes[0].nodeValue + "</span><br/>";
	brMiscTxt += "<span style='font-size:14px;font-weight:bold;'>" + x[bRight].getElementsByTagName('MISCTXT')[0].getElementsByTagName('PRICE')[k].childNodes[0].nodeValue + "</span> ";
	brMiscTxt += "<span style='font-size:10px;'>" + x[bRight].getElementsByTagName('MISCTXT')[0].getElementsByTagName('UNIT')[k].childNodes[0].nodeValue + "</span><br/><br/>";

}


  document.getElementById('topBigImage').src = x[top].getElementsByTagName('BIGIMG')[0].childNodes[0].nodeValue;
  document.getElementById('topBigImage').alt = x[top].getElementsByTagName('ALTTAGINFO')[0].childNodes[0].nodeValue;
  document.getElementById('topBILink').href = x[top].getElementsByTagName('LINK')[0].childNodes[0].nodeValue;
  document.getElementById('infoBox1').innerHTML = "<span style='font-weight:bold;font-size:14px;'>" + x[top].getElementsByTagName('TITLE')[0].childNodes[0].nodeValue + "</span><br/><span style='font-size:10px;'>" + x[top].getElementsByTagName('SKU')[0].childNodes[0].nodeValue + "</span><br/><br/>" + x[top].getElementsByTagName('TEXT')[0].childNodes[0].nodeValue + "<br/><br/>";
  document.getElementById('infoBox1').innerHTML += topMiscTxt;   


  document.getElementById('firstLowerImage').src = x[bLeft].getElementsByTagName('SMIMG')[0].childNodes[0].nodeValue;
  document.getElementById('firstLowerImage').alt = x[bLeft].getElementsByTagName('ALTTAGINFO')[0].childNodes[0].nodeValue;
  document.getElementById('btmLBILink').href = x[bLeft].getElementsByTagName('LINK')[0].childNodes[0].nodeValue;
  document.getElementById('infoBox2').innerHTML = "<span style='font-weight:bold;font-size:14px;'>" + x[bLeft].getElementsByTagName('TITLE')[0].childNodes[0].nodeValue + "</span><br/><span style='font-size:10px;'>" + x[bLeft].getElementsByTagName('SKU')[0].childNodes[0].nodeValue + "</span><br/><br/>" + x[bLeft].getElementsByTagName('TEXT')[0].childNodes[0].nodeValue + "<br/><br/>";
  document.getElementById('infoBox2').innerHTML += blMiscTxt;
  
  document.getElementById('secondLowerImage').src = x[bRight].getElementsByTagName('SMIMG')[0].childNodes[0].nodeValue;
  document.getElementById('secondLowerImage').alt = x[bRight].getElementsByTagName('ALTTAGINFO')[0].childNodes[0].nodeValue;
  document.getElementById('btmRBILink').href = x[bRight].getElementsByTagName('LINK')[0].childNodes[0].nodeValue;
  document.getElementById('infoBox3').innerHTML = "<span style='font-weight:bold;font-size:14px;'>" + x[bRight].getElementsByTagName('TITLE')[0].childNodes[0].nodeValue + "</span><br/><span style='font-size:10px;'>" + x[bRight].getElementsByTagName('SKU')[0].childNodes[0].nodeValue + "</span><br/><br/>" + x[bRight].getElementsByTagName('TEXT')[0].childNodes[0].nodeValue + "<br/><br/>";
  document.getElementById('infoBox3').innerHTML += brMiscTxt;
  
} 
function getArgs() {
  var args = new Object();
  var query = location.search.substring(1);
  var pairs = query.split("&");
  for(var i = 0; i < pairs.length; i++) {
    var pos = pairs[i].indexOf('=');
	if (pos == -1) continue;
	var argname = pairs[i].substring(0,pos);
	var value = pairs[i].substring(pos+1);
	args[argname] = unescape(value);
  }
  if (isNaN(args.top) && isNaN(args.bL) && isNaN(args.bR)) {
    var lengthTop = x.length;
    displayInformation(lengthTop-1, lengthTop-2, lengthTop-3);
  } else {
    displayInformation(args.top, args.bL, args.bR);
  }
}


